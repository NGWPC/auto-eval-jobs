AWS_DEFAULT_REGION=us-east-1
GDAL_CACHEMAX=1024
DASK_CLUST_MAX_MEM=7GiB

# GDAL Configuration
GDAL_NUM_THREADS=1
GDAL_TIFF_DIRECT_IO=YES
GDAL_DISABLE_READDIR_ON_OPEN=TRUE
CPL_LOG_ERRORS=ON
CPL_VSIL_CURL_ALLOWED_EXTENSIONS=.tif,.vrt
VSI_CACHE_SIZE=268435456
CPL_VSIL_USE_TEMP_FILE_FOR_RANDOM_WRITE=YES

# Processing Configuration
RASTERIO_CHUNK_SIZE=2048
DEFAULT_WRITE_BLOCK_SIZE=4096
COG_BLOCKSIZE=512
COG_OVERVIEW_LEVEL=4

# Nodata Values
EXTENT_NODATA_VALUE=255
DEPTH_NODATA_VALUE=-9999
INUNDATION_NODATA_VALUE=255

# Output Configuration
MOSAIC_BLOCK_SIZE=512
MOSAIC_COMPRESS_TYPE=LZW
MOSAIC_PREDICTOR=2
INUNDATION_COMPRESS_TYPE=lzw
INUNDATION_BLOCK_SIZE=256

# Processing Defaults
DEFAULT_CLIP_OPERATION=exclude
LAKE_ID_FILTER_VALUE=-999
DEFAULT_METRICS_CHUNK_SIZE=1024

# Logging
LOG_SUCCESS_LEVEL_NUM=25

